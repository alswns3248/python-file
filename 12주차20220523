import random

computer = random.choice(["가위","바위","보"])
user = input("입력 : ")
print("컴퓨터 : ", computer)

if user == "바위" or user == "가위" or user == "보":
    if user == computer :
        print("비김")
    elif(user == "바위" and computer == "가위")or \
        (user == "가위" and computer == "보") or \
        (user == "보" and computer == "바위"):
        print("이김")
    else :
        print("짐")
else :
    print("가위, 바위, 보 중에 입력")
